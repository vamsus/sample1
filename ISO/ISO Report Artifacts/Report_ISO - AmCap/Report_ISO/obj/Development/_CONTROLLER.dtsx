        <DTS:Executable
          DTS:refId="Package\SEQ - Load TX Commercial Data\SQL Load TX Commercial Premium tables"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
